{
  "compilerOptions": {
      "lib": ["dom", "es5", "es2015"],
      "target": "es5",
      "strict": false,
      "module": "es2015",
      "moduleResolution": "node",
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": false,
      "noImplicitThis": false,
      "allowJs": false,
      "baseUrl": ".",
      "paths": {
        "@/*": ["src/*"]
	    }
  }
}
